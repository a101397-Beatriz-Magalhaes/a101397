    def acimaMedia(n):
    i = 0
    res = 0
    media = 0
    lista1 = []
    numeroTotal = 0
    while i < n:
        x = random.randint(-1000, 1000)
        lista1.append(x)
        i = i + 1
    for numero in lista1:
        numeroTotal = numeroTotal + numero
    media = numeroTotal / n
    for n in lista1:
        if n > media:
            res = res + 1
    return res
    
