    def figuais(f1, f2):
    var = False
    i = 0
    listaA = []
    listaB = []
    file1 = open ("texto1.txt", "r")
    for linha in file1:
        listaA.append(linha)
    print(listaA)
    file2 = open ("texto2.txt", "r")
    for linha in file2:
        listaB.append(linha)
    print(listaB)
    while i < len(listaA):
        if listaA[i] == listaB[i]:
            var = True
        else:
            var = False
        i = i + 1
    return var
    
