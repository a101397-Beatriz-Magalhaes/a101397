   def CriaF(l1,l2):
    l3 = []
    for n in l1:
        l3.append(n)
    for n in l2:
        l3.append(n)
    print(l3)
    return l3

def merge(l1, l2):
    l3 = CriaF(l1,l2)
    i = 1
    j = 0
    while j < len(l3):
        while i < len(l3):
            if l3[i - 1] > l3[i]:
                l3[i], l3[i - 1] = l3[i-1], l3[i]
            i = i + 1
        j = j + 1
    return l3
    
